VITE_API_BASE_URL=http://localhost:8000/api
VITE_API_TIMEOUT=15000

# Authentication settings
VITE_AUTH_ENABLED=true

# WebSocket configuration for real-time updates
VITE_WEBSOCKET_URL=ws://localhost:8000/ws
VITE_REFRESH_INTERVAL=30000

# Logging and debugging
VITE_LOG_LEVEL=info
VITE_ENABLE_MOCK_API=false
VITE_NODE_ENV=development

# Analytics configuration
VITE_GOOGLE_ANALYTICS_ID=

# Feature flags
VITE_FEATURE_FLAG_ENABLE_WEBSOCKETS=true
VITE_FEATURE_FLAG_ENABLE_NOTIFICATIONS=true
VITE_FEATURE_FLAG_ENABLE_DARK_MODE=true
VITE_FEATURE_FLAG_ENABLE_ANALYTICS=false
VITE_FEATURE_FLAG_ENABLE_ADVANCED_CHARTS=true
VITE_FEATURE_FLAG_ENABLE_DEBUG_TOOLS=false