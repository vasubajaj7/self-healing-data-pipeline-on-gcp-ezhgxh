TEST_ENV=development
TEST_LOG_LEVEL=INFO
TEST_OUTPUT_DIR=./reports
TEST_DATA_DIR=./src/test/mock_data
TEST_TIMEOUT=60

# Google Cloud Platform Test Settings
TEST_GCP_PROJECT_ID=your-test-project-id
TEST_GCP_REGION=us-central1
TEST_GCP_ZONE=us-central1-a
TEST_GCP_SERVICE_ACCOUNT_KEY_PATH=./test-service-account-key.json

# Test Resource Naming
TEST_RESOURCE_PREFIX=shp-test
TEST_ENVIRONMENT_ID=unique-test-id
TEST_AUTO_DESTROY=true
TEST_ENVIRONMENT_TTL=24

# BigQuery Test Settings
TEST_BIGQUERY_DATASET=shp_test_data
TEST_BIGQUERY_LOCATION=US

# Cloud Storage Test Settings
TEST_GCS_BUCKET=shp-test-data-bucket
TEST_GCS_STAGING_BUCKET=shp-test-staging-bucket

# Cloud Composer Test Settings
TEST_COMPOSER_ENVIRONMENT=shp-test-composer
TEST_COMPOSER_LOCATION=us-central1
TEST_ENABLE_COMPOSER=false

# Vertex AI Test Settings
TEST_VERTEX_AI_LOCATION=us-central1
TEST_MODEL_DISPLAY_NAME=shp-test-model
TEST_ENDPOINT_DISPLAY_NAME=shp-test-endpoint
TEST_ENABLE_VERTEX_AI=false

# Test Data Generation Settings
TEST_DATA_SIZE=small
TEST_DATA_SCHEMA_PATH=./src/test/mock_data/bigquery/sample_schema.json
TEST_GENERATE_ANOMALIES=true
TEST_ANOMALY_RATE=0.05

# Mock Service Settings
TEST_USE_MOCKS=true
TEST_MOCK_EXTERNAL_APIS=true
TEST_MOCK_GCP_SERVICES=true

# Performance Test Settings
TEST_PERFORMANCE_ITERATIONS=100
TEST_PERFORMANCE_CONCURRENCY=10
TEST_PERFORMANCE_DURATION=60

# E2E Test Settings
TEST_BROWSER=chromium
TEST_HEADLESS=true
TEST_BASE_URL=http://localhost:3000
TEST_API_URL=http://localhost:8000

# Test Notification Settings
TEST_TEAMS_WEBHOOK_URL=https://outlook.office.com/webhook/your-test-webhook-url
TEST_EMAIL_RECIPIENTS=test@example.com

# Test Authentication Settings
TEST_AUTH_ENABLED=false
TEST_AUTH_USERNAME=test-user
TEST_AUTH_PASSWORD=test-password

# Test Coverage Settings
TEST_COVERAGE_THRESHOLD=85
TEST_COVERAGE_EXCLUDE=src/test/*,*/__pycache__/*,*/migrations/*

# CI/CD Integration Settings
CI=false
CI_PIPELINE_ID=
CI_JOB_ID=
CI_COMMIT_SHA=