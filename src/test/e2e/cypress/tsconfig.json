{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "moduleResolution": "node",
    "types": ["cypress", "node", "@testing-library/cypress", "@cypress/code-coverage"],
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "preserve",
    "strict": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@fixtures/*": ["fixtures/*"],
      "@support/*": ["support/*"],
      "@utils/*": ["../../../utils/*"]
    }
  },
  "include": [
    "**/*.ts",
    "support/*.ts",
    "fixtures/**/*.json",
    "integration/**/*.spec.ts",
    "plugins/**/*.ts",
    "node_modules/cypress/types/index.d.ts"
  ],
  "exclude": ["node_modules"]
}