APP_ENV=development
APP_DEBUG=True
APP_LOG_LEVEL=INFO
APP_CONFIG_PATH=./configs/default_config.yaml

# Google Cloud Platform Settings
GCP_PROJECT_ID=your-gcp-project-id
GCP_LOCATION=us-central1
GCP_SERVICE_ACCOUNT_KEY_PATH=./service-account-key.json

# BigQuery Settings
BIGQUERY_DATASET=self_healing_pipeline
BIGQUERY_LOCATION=us-central1
BIGQUERY_RESERVATION_ID=

# Cloud Storage Settings
GCS_BUCKET_PREFIX=self-healing-pipeline
GCS_STAGING_BUCKET=self-healing-pipeline-staging
GCS_DATA_BUCKET=self-healing-pipeline-data
GCS_BACKUP_BUCKET=self-healing-pipeline-backup

# Cloud Composer Settings
COMPOSER_ENVIRONMENT=self-healing-pipeline
COMPOSER_LOCATION=us-central1

# Vertex AI Settings
VERTEX_AI_LOCATION=us-central1
VERTEX_AI_MODEL_REGISTRY=self-healing-models
VERTEX_AI_ENDPOINT_ID=

# Self-Healing Settings
SELF_HEALING_MODE=SEMI_AUTOMATIC
SELF_HEALING_CONFIDENCE_THRESHOLD=0.85
SELF_HEALING_MAX_RETRY_ATTEMPTS=3

# Database Settings
FIRESTORE_COLLECTION_PREFIX=self_healing_pipeline

# Notification Settings
# Microsoft Teams Webhook URLs
TEAMS_WEBHOOK_URL_CRITICAL=https://outlook.office.com/webhook/your-webhook-url-for-critical-alerts
TEAMS_WEBHOOK_URL_DEFAULT=https://outlook.office.com/webhook/your-webhook-url-for-default-alerts

# Email Notification Settings
EMAIL_SMTP_SERVER=smtp.example.com
EMAIL_SMTP_PORT=587
EMAIL_USE_TLS=True
EMAIL_USERNAME=alerts@example.com
EMAIL_PASSWORD=your-email-password
EMAIL_SENDER=Self-Healing Pipeline <alerts@example.com>
EMAIL_RECIPIENTS=admin@example.com,engineer@example.com

# Monitoring Settings
MONITORING_ALERT_THRESHOLD_CPU=80
MONITORING_ALERT_THRESHOLD_MEMORY=85
MONITORING_ALERT_THRESHOLD_DISK=90
MONITORING_METRICS_EXPORT_INTERVAL=60

# API Settings
API_HOST=0.0.0.0
API_PORT=8000
API_DEBUG=True
API_SECRET_KEY=generate-a-secure-random-key
API_CORS_ORIGINS=*

# External Service Integration
EXTERNAL_API_KEY=your-external-api-key
EXTERNAL_API_ENDPOINT=https://api.example.com/v1

# Development Settings (ignored in production)
DEV_MOCK_SERVICES=False
DEV_SAMPLE_DATA=False